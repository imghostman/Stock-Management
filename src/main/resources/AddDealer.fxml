<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" fx:id="AddDealerMainPane" prefHeight="815.0" prefWidth="808.0"
            style="-fx-background-color: #16161d;" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="EmergingWorld.Controller.AddDealerController">
    <children>
        <Pane fx:id="AddDealerTopPane" prefHeight="200.0" prefWidth="807.0">
            <children>
                <ImageView fitHeight="219.0" fitWidth="807.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@EmergingWorld/images/Tdealer.jpg" />
                    </image>
                </ImageView>
            </children>
        </Pane>
        <Pane fx:id="AddDealerBottomPane" layoutY="205.0" prefHeight="608.0" prefWidth="807.0">
            <children>
                <TextField fx:id="DealerMobNo" layoutX="319.0" layoutY="340.0" prefHeight="44.0" prefWidth="407.0" promptText="Mobile No.">
                    <font>
                        <Font name="Abyssinica SIL" size="20.0" />
                    </font>
                </TextField>
                <ComboBox fx:id="DealerGenderDrop" layoutX="319.0" layoutY="179.0" prefHeight="39.0" prefWidth="407.0"
                          promptText="SELECT" style="-fx-background-color: cyan;" visibleRowCount="2" />
                <Label fx:id="DealerGender" layoutX="35.0" layoutY="179.0" prefHeight="39.0" prefWidth="90.0" text="GENDER" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="Abyssinica SIL" size="18.0" />
                    </font>
                </Label>
                <TextField fx:id="DealerName" layoutX="319.0" layoutY="94.0" prefHeight="44.0" prefWidth="407.0" promptText="Name">
                    <font>
                        <Font name="Abyssinica SIL" size="20.0" />
                    </font>
                </TextField>
                <Button fx:id="SubmitButton" layoutX="522.0" layoutY="531.0" onAction="#SubmitAction"
                        onMouseEntered="#entered" onMouseExited="#exited" prefHeight="51.0" prefWidth="202.0"
                        style="-fx-background-color: #9375ff;" text="SUBMIT" textFill="WHITE">
                    <font>
                        <Font name="Abyssinica SIL" size="24.0" />
                    </font>
                </Button>
                <TextField fx:id="DealerTinNo" layoutX="319.0" layoutY="433.0" prefHeight="44.0" prefWidth="407.0" promptText="Tax Identification Number">
                    <font>
                        <Font name="Abyssinica SIL" size="20.0" />
                    </font>
                </TextField>
                <TextField fx:id="DealerAddress" layoutX="319.0" layoutY="260.0" prefHeight="44.0" prefWidth="407.0" promptText="Address">

                    <font>
                        <Font name="Abyssinica SIL" size="20.0" />
                    </font>
                </TextField>
                <Button layoutX="6.0" layoutY="2.0" mnemonicParsing="false" onAction="#onBack"
                        onMouseEntered="#entered" onMouseExited="#exited" prefHeight="25.0" prefWidth="58.0" style="-fx-background-color: red;" text="&lt;Back">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
            <Label layoutX="35.0" layoutY="94.0" prefHeight="44.0" prefWidth="189.0" text="Name" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label layoutX="36.0" layoutY="260.0" prefHeight="44.0" prefWidth="213.0" text="Address" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label layoutX="35.0" layoutY="340.0" prefHeight="44.0" prefWidth="189.0" text="Mobile Number" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label layoutX="35.0" layoutY="433.0" prefHeight="44.0" prefWidth="189.0" text="Tax Identification No." textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Button fx:id="clearButton" layoutX="130.0" layoutY="531.0" mnemonicParsing="false"
                    onAction="#onClear" prefHeight="51.0" prefWidth="202.0" style="-fx-background-color: #bf4646;" text="Clear" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Label layoutX="257.0" layoutY="15.0" prefHeight="70.0" prefWidth="308.0" text="Dealer Details" textFill="WHITE">
               <font>
                  <Font name="Forte" size="48.0" />
               </font>
            </Label>
            </children>
        </Pane>

    </children>
</AnchorPane>
